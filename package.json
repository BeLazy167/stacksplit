{
  "name": "stacksplit",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "generate-themes": "tamagui generate-themes ./utils/theme-builder.ts ./utils/theme-output.ts"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.2.34",
    "@clerk/clerk-react": "^5.16.1",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.1.7",
    "@tamagui/cli": "^1.116.15",
    "@tamagui/config": "^1.116.12",
    "@tamagui/core": "^1.116.12",
    "@tamagui/lucide-icons": "^1.116.12",
    "@tanstack/react-query": "^5.61.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "expo": "^52.0.7",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.1",
    "expo-dev-launcher": "^4.0.22",
    "expo-file-system": "~18.0.4",
    "expo-image-picker": "~16.0.2",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.5",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "expo-web-browser": "~14.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~4.0.0",
    "react-native-web": "~0.19.10",
    "tamagui": "^1.116.12",
    "expo-image": "~2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^15.1.2",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
